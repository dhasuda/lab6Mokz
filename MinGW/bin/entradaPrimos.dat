Primos {{{
var {logic achou; int n, div, resto, cont, num;}
statements {
	read (n);
   	if n > 0 then {
		num := 1; cont := 0;
		while cont < n do {
			num := num + 1; div := 2; achou := false;
			while achou = false && div * div <= num do {
				resto := num % div;
				if resto = 0 then achou := true;
				else div := div + 1;
			}
			if achou = false then {
				write (num); cont := cont + 1;
			}
		}
	}
}
}}}
